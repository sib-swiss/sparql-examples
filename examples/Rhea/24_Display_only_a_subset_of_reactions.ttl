@prefix ex: <https://sparql.rhea-db.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix spex: <https://purl.expasy.org/sparql-examples/ontology#> .

ex:24_Display_only_a_subset_of_reactions a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Select the 10 reactions, with the lowest identifier by alphabetic sort"@en ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """PREFIX rh:<http://rdf.rhea-db.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT 
  ?reaction
  ?reactionId
WHERE {
  ?reaction rdfs:subClassOf rh:Reaction .
  ?reaction rh:id ?reactionId . 
}
ORDER BY ?reaction
""" ;
    schema:target <https://sparql.rhea-db.org/sparql/> .
