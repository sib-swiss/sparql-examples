@prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ex:NXQ_00107 a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Proteins with a protein existence not \"At protein level\" (PE=1) and with a HGNC approved gene name that contains \"orf\"" ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """PREFIX : <http://nextprot.org/rdf/>
PREFIX db: <http://nextprot.org/rdf/db/>

SELECT DISTINCT ?entry WHERE {
 ?entry :existence ?level.
 filter (?level != :Evidence_at_protein_level).
 ?entry :reference ?r.
 ?r :provenance db:HGNC ; :accession ?ac.
 filter (regex(?ac,'orf')) .
}""" ;
    schema:keyword "gene",
        "protein existence",
        "tutorial" ;
    schema:target <https://sparql.nextprot.org/sparql> .

