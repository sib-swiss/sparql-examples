@prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ex:NXQ_00007 a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Proteins associated with diseases that are associated with cardiovascular aspects" ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """PREFIX : <http://nextprot.org/rdf/>
PREFIX nextprot_cv: <http://nextprot.org/rdf/terminology/>

select distinct ?entry where {
 ?entry :isoform /:medical / :term /:related / :childOf nextprot_cv:D002318.
}""" ;
    schema:keyword "disease",
        "tutorial" ;
    schema:target <https://sparql.nextprot.org/sparql> .

