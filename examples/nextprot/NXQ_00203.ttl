prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
prefix schema:<https://schema.org/>
ex:NXQ_00203
schema:target <https://sparql.nexprot.org/sparql> ;
 sh:prefixes _:sparql_examples_prefixes ;
  a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
  rdfs:comment '''Proteins with a mature chain of from 300 to 400 residues''' ;
 schema:keyword "processing";
 schema:keyword "sequence";
 schema:keyword "tutorial";
  sh:select '''select distinct ?entry where {
 ?entry :isoform ?iso.
 ?iso :matureProtein [ :start ?mstart ; :end ?mend].
 filter (?mend-?mstart > 0). # chain fully defined
 bind(?mend - ?mstart as ?mlen)	 
 filter(?mlen >= 300 && ?mlen <= 400)
}'''
.
