@prefix ex:<https://purl.expasy.org/sparql-examples/ontology#neXtProt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
ex:NXQ_09448 a sh:SPARQLExecutable, sh:SPARQLSelectExecutable ;
  sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Check there are no entries with a keyword in the category "Coding sequence diversity" that does not apply to human proteins'''@en ;
  schema:keywords "QC", "keyword" ;
  schema:target <https://sparql.nextprot.org/sparql> ;
  sh:select '''PREFIX : <http://nextprot.org/rdf/>
PREFIX cv: <http://nextprot.org/rdf/terminology/>

select distinct ?entry where {  # Include ?kw if result is not 0 to identify problem keyword
  values ?kw {
  cv:KW-0669 # Pyrrolysine
  cv:KW-1197 # Ribosomal skipping
  cv:KW-1159 # RNA suppression of termination
  cv:KW-1158 # RNA termination-reinitiation
  cv:KW-1156 # RNA translational shunting
  }
  ?entry :isoform /:keyword /:term ?kw.
}''' .
