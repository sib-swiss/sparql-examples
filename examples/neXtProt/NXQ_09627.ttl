@prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
ex:NXQ_09627 a sh:SPARQLExecutable, sh:SPARQLSelectExecutable ;
  sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Check there are no entries with a peptide mapping annotation with evidence from PeptideAtlas but no xref to PeptideAtlas''' ;
  schema:keyword "QC", "mass spectrometry", "peptide", "proteomics", "cross-reference" ;
  schema:target <https://sparql.nextprot.org/sparql> ;
  sh:select '''PREFIX : <http://nextprot.org/rdf/>
PREFIX cv: <http://nextprot.org/rdf/terminology/>
PREFIX db: <http://nextprot.org/rdf/db/>
 
select distinct ?entry where {
  ?entry a :Entry.
  ?entry :isoform /:peptideMapping /:evidence /:assignedBy ?src.
  filter (regex (?src,'PeptideAtlas'))
  filter not exists {?entry :reference /:provenance db:PeptideAtlas.}
}''' .
