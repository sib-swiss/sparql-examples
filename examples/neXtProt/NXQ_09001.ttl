@prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
ex:NXQ_09001 a sh:SPARQLExecutable, sh:SPARQLSelectExecutable ;
  sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Check there are no protein families with no entry associated''' ;
  schema:keyword "QC", "family", "vocabulary", "snorql-only" ;
  schema:target <https://sparql.nextprot.org/sparql> ;
  sh:select '''PREFIX : <http://nextprot.org/rdf/>
PREFIX cv: <http://nextprot.org/rdf/terminology/>

select distinct ?famac where {
  ?fam a :FamilyInfo .
  ?fam :accession ?famac .
  filter not exists {?member :family / :accession ?famac .}
}
order by ?famac''' .
