@prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ex:NXQ_00230 a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Proteins with experimentally determined lengthy alpha-helices (> 75 aa)" ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """PREFIX : <http://nextprot.org/rdf/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?entry (str(?gen) as ?gene) ?hlen WHERE {
 ?entry :isoform ?iso.
 ?entry :gene / :recommendedName / rdfs:label ?gen.
 ?iso :helix ?hel .
 ?hel :start ?s; :end ?e .
 bind((?e - ?s + 1) as ?hlen )
 filter(?hlen > 75)
}
order by desc(?hlen)""" ;
    schema:keyword "3D structure",
        "method",
        "sequence",
        "tutorial" ;
    schema:target <https://sparql.nextprot.org/sparql> .

