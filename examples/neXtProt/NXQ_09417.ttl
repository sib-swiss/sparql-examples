@prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
ex:NXQ_09417 a sh:SPARQLExecutable, sh:SPARQLSelectExecutable ;
  sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Check there are no entries whose gene is not located on the plus or minus strand or where the strand is unknown'''@en ;
  schema:keywords "QC", "chromosomal location" ;
  schema:target <https://sparql.nextprot.org/sparql> ;
  sh:select '''PREFIX : <http://nextprot.org/rdf/>
PREFIX cv: <http://nextprot.org/rdf/terminology/>

select distinct ?entry ?strand where {
  ?entry :gene / :strand ?strand .
   filter (!regex (?strand, '1'))
   filter (!regex (?strand, '-1'))
   filter (!regex (?strand, '0'))
}''' .
