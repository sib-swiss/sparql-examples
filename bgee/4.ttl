@prefix ex: <https://www.bgee.org/sparql/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ex:4 a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "What are the anatomical entities where the APOC1 Homo sapiens gene is expressed?" ;
    sh:prefixes _:sparql_examples_prefixes ;
    schema:target <https://www.bgee.org/sparql/> ;
    sh:select """
SELECT DISTINCT ?anat ?anatName WHERE {
    ?seq a orth:Gene ;
        genex:isExpressedIn ?anat ;
        rdfs:label "APOC1" .
    ?anat a genex:AnatomicalEntity ;
        rdfs:label ?anatName .
    ?seq orth:organism ?organism .
    ?organism obo:RO_0002162  ?species .
    ?species a up:Taxon ;
        up:scientificName "Homo sapiens" .
}
""" .

